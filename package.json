{
  "name": "ai-agent-system",
  "version": "1.0.0",
  "description": "AI Agent System with Express backend and n8n integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cap:init": "cap init ai-agent com.ai.agent --web-dir=public",
